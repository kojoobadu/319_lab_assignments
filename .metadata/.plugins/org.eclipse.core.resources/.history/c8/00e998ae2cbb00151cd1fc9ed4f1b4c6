package swing;

import java.util.Scanner;

public class apples {
	
	
	public static void main(String args[]){
		Scanner input = new Scanner(System.in);
		double a = 0;
		double b = 0;
		double c = 0;
		
		double x1 = 0;
		double x2 = 0;
		
		System.out.println("Enter a float value for a :");
		a = input.nextFloat();
		System.out.println("Enter a float value for b :");
		b = input.nextFloat();
		System.out.println("Enter a float value for c :");
		c = input.nextFloat();
		
		x1 = (-b + Math.sqrt((b*b) - (4*a*c)))/ (2*a*c);
		x2 = (-b - Math.sqrt((b*b) - (4*a*c)))/ (2*a*c);
		
		System.out.println("The value of x1 is " + x1 );
		
	}
	
	
}
